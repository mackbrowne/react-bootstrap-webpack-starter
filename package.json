    "firebase": "^8.6.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss-normalize": "^10.0.1",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^17.0.2",
